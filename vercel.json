{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/cursos",
        "dest": "/server/index.js"
      },
      {
        "src": ".*",
        "dest": "/404.html"
      }
    ]
  }
  